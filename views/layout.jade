!!! 5
html
	head
		title Volox Node test
		link(rel='stylesheet', href='/stylesheets/style.css')
		script(src='/scripts/jquery.js')
		script(src='/scripts/script.js')
	body
		header
			img(src='/img/logo.jpg', title='Logo', alt='Logo')
			h1 #{title}
		nav
			ul
				- var items={ "Home": "/", "HTML5": "/html5", "CSS3": "/css3" }
				- each url,title in items
					li: a(href='#{url}', title='#{title}') #{title}
		section.content!=body
		footer
			span Powered by Volox
			img(src="/img/smallLogo.jpg",title="Volox",alt="Volox")